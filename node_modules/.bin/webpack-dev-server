#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0/node_modules/webpack-dev-server/bin/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0/node_modules/webpack-dev-server/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/.pnpm/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules:/home/nxtwave/bmi-calculator/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/webpack-dev-server/bin/node_modules:/home/nxtwave/bmi-calculator/bmi-calculator/node_modules/webpack-dev-server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else 
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
